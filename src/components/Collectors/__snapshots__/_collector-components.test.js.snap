exports[`CollectorConfiguration component > snapshots > should match snapshot 1`] = `
<div>
  <p>
    To edit the 
    <code>
      go.d/example.conf
       
    </code>
     configuration file:
  </p>
  <ol>
    <li>
      Navigate to the
       
      <a
        href="/docs/configure/nodes#the-netdata-config-directory"
      >
        Netdata config directory
      </a>
      .
      <pre
        class="language-bash"
        data-language="bash"
        data-testid="code-block"
      >
        <code>
          cd /etc/netdata
        </code>
      </pre>
    </li>
    <li>
      Use the
       
      <a
        href="/docs/configure/nodes#use-edit-config-to-edit-configuration-files"
      >
        <code>
          edit-config
        </code>
      </a>
       
      script to edit 
      <code>
        go.d/example.conf
      </code>
      .
      <pre
        class="language-bash"
        data-language="bash"
        data-testid="code-block"
      >
        <code>
          sudo ./edit-config 
          go.d/example.conf
        </code>
      </pre>
    </li>
    <li>
      Enable changes to the collector by setting 
      <code>
        enabled: yes
      </code>
      .
    </li>
    <li>
      Save the changes and restart the Agent with
       
      <code>
        sudo systemctl restart netdata
      </code>
       or the
       
      <a
        href="/docs/configure/start-stop-restart"
      >
        appropriate method
      </a>
       
      for your system.
    </li>
  </ol>
  <p>
    For all available options, please see the module
     
    <a
      href="{configURL}"
    >
      configuration file
    </a>
    .
  </p>
</div>
`;

exports[`CollectorDebug component > snapshots > should match snapshot 1`] = `
<div>
  <p>
    To troubleshoot issues with the 
    <code>
      example
    </code>
     collector, run the 
    <code>
      go.d.plugin
    </code>
     with the debug option enabled.
  </p>
  <ol>
    <li>
      Navigate to your plugins directory, usually at
      <code>
        /usr/libexec/netdata/plugins.d/
      </code>
      . If that's not the case on your system, open 
      <code>
        netdata.conf
      </code>
       and look for the setting 
      <code>
        plugins directory
      </code>
      .
       
    </li>
    <li>
      In the plugins directory, switch to the 
      <code>
        netdata
      </code>
       user.
      <pre
        class="language-bash"
        data-language="bash"
        data-testid="code-block"
      >
        <code>
          cd /usr/libexec/netdata/plugins.d/ sudo -u netdata -s
        </code>
      </pre>
       
    </li>
    <li>
      Run the 
      <code>
        go.d.plugin
      </code>
       in debug mode to identify issues:
      <pre
        class="language-bash"
        data-language="bash"
        data-testid="code-block"
      >
        <code>
          ./
          go.d.plugin
           -d -m 
          example
        </code>
      </pre>
      <p>
        The output should give you clues as to why the collector isn't working.
      </p>
    </li>
  </ol>
</div>
`;
